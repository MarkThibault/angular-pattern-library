    "angular-modal": "git://github.com/MarkThibault/angular-modal.git",
    "angular-session-timeout": "git://github.com/MarkThibault/angular-session-timeout.git",
    "angular-toast-messaging": "git://github.com/MarkThibault/angular-toast-messaging.git"
  },
  "devDependencies": {
    "angular-shared-scss": "git://github.com/MarkThibault/angular-shared-scss.git",
