    "registry": "https://github.com/MarkThibault/angular-pattern-library.git"
  },
  "dependencies": {
    "@types/angular": "^1.6.7",
    "@types/node": "^7.0.5",
    "angular": "1.5.8",
    "angular-animate": "1.5.8",
